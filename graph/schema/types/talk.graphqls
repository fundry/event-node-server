type Talk {
    id : Int!
    title: String!
    talkCoverUri: String
    duration : Int!
    summary: String!
    description: String!
    Archived: Boolean!
    tags: [String]
    createdAt: Time!
    updatedAt: Time!
    event_id : Int
    speaker_id: Int!
    event: [Event]
    speaker: [User]
}

input CreateTalk {
    title: String!
    talkCoverUri: String
    summary: String!
    description: String!
    Archived: Boolean!
    duration : Int!
    tags: [String]
}

input UpdateTalk {
    title: String!
    talkCoverUri: String
    summary: String!
    description: String!
    Archived: Boolean!
    duration : Int!
    tags: [String]
}