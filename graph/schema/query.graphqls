type Query {
    event(id: Int name: String!) : Event!
    events: [Event!]!

    user(id: Int name: String!) : User!
    users : [User!]!

    preference(id: Int name: String!) : Preference!
    preferences : [Preference!]!

    file(id: Int name: String!) : File!
    files: [File!]!

    team(id: Int name: String!): Team!
    teams: [Team!]!
}