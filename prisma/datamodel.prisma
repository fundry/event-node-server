type User {
  id: ID! @id
  name: String! @unique
  email: String! @unique
  password: String! @unique
  bucketLink: String
}

type Event {
  id: ID! @id
  name: String! @unique
  description: String
  duration: Int
  organizer: String
  type: String!
  venue: String!
  date: Int 
  supportEmail: String! @unique
  website: String
  password : String!
  createdAt: DateTime! @createdAt
  attendees: Int
  teams: Int
  bucketLink: String
}

type Attendee {
  id: ID! @id
  event: Event @relation( name : "EventAttendee"   )
}

type EventTeam {
    id: ID! @id
   name: String!   @unique
   description: String!
  createdAt: DateTime! @createdAt
}

type File {
    id: ID! @id
  filename: String
  mimetype: String
  size: String
  uri: String @unique
  encoding: String
  timestamp: Int
}